connect '/C=5'
setbaud 115200

inputbox 'pwd' 'please pwd'
pwd = inputstr
sendln 'SKSETPWD C ' pwd
waitln 'OK'
inputbox 'bid' 'please bid'
bid = inputstr
sendln 'SKSETRBID' bid
waitln 'OK'


